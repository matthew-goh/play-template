# An example controller showing a sample home page
GET     /                           controllers.HomeController.index()
GET     /api     controllers.ApplicationController.index
GET     /api/create     controllers.ApplicationController.create
GET     /api/read/:id     controllers.ApplicationController.read(id: String)
GET     /api/update/:id     controllers.ApplicationController.update(id: String)
GET     /api/delete/:id     controllers.ApplicationController.delete(id: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
